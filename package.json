        "undici": "^5.0.0"
    },
    "devDependencies": {
        "chai": "^4.3.6",
        "chai-http": "^4.3.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "mocha": "^9.2.2",
        "nodemon": "^2.0.15",
        "pino-pretty": "^7.6.0",
        "prettier": "^2.6.2",
        "sequelize-cli": "^6.4.1",
        "sinon": "^13.0.1",
        "sqlite3": "^5.0.2"
    },
    "scripts": {
        "start": "NODE_ENV=production node -r dotenv-flow/config src/index.js",
