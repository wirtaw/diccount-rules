{
    "name": "shop-api",
    "version": "1.0.0",
    "description": "Shop API",
    "main": "src/index.js",
    "engines": {
        "node": ">=14.10.0"
    },
    "dependencies": {
        "dotenv-flow": "^3.2.0",
        "fastify": "^3.27.4",
        "fastify-oas": "^3.0.8",
        "fastify-plugin": "^3.0.1",
        "fastify-sequelize": "^1.0.4",
        "luxon": "^2.3.1",
        "pg": "^8.7.3",
